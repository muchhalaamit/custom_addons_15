<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<record model="ir.ui.view" id="product_template_brand_form_view">
		<field name="name">product.template.brand.form.view</field>
		<field name="model">product.template</field>
		<field name="inherit_id" ref="product.product_template_form_view"/>
		<field name="arch" type="xml">
			<xpath expr="//notebook/page[@name='general_information']/group/group[@name='group_general']/field[@name='active']" position="after">
				<field name="is_branding"/>
			</xpath>
		</field>
	</record>
</odoo>
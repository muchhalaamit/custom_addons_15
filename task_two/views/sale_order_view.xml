<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="sale_order_other_info_form_view" model="ir.ui.view">
        <field name="name">sale.order.other.info.form.view</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='order_lines']/field[@name='order_line']/tree/field[@name='name']" position="after">
                <field name="other_information"/>
            </xpath>
        </field>
    </record>

</odoo>